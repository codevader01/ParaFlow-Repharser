{
    "manifest_version": 3,
    "name": "Paraflow Rephraser",
    "version": "1.0",
    "description": "Rephrase text using AI.",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "logo48.png",
        "48": "logo48.png",
        "128": "logo128.png"
      }
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "host_permissions": [
      "http://127.0.0.1:8000/*"
    ]
  }
  